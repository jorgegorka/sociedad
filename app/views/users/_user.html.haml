%tr
  %td{class: td_class.render}
    = user.name
  %td{class: td_class.render}
    = user.username
  %td{class: td_class.render}
    = user.email
  %td{class: td_class.render}
    = render partial: 'shared/button_edit', locals:  {destination: edit_user_path(user)}
    = render partial: 'shared/button_delete', locals:  {destination: {model: user} }